{
  "name": "rewards-catalog",
  "version": "0.0.1",
  "description": "UI repo for rewards-catalog",
  "repository": {
    "type": "git",
    "url": "https://github.com/Capillary/rewards-catalog-ui"
  },
  "engines": {
    "npm": ">=5 ",
    "node": ">=12.0.0 <15.0.0"
  },
  "author": "keshav.agrawal@capillarytech.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Capillary/rewards-catalog-ui/issues"
  },
  "scripts": {
    "extract-intl": "node ./internals/scripts/babel-node ./internals/scripts/extract-intl.js && cp app/translations/en.json dist",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean && npm run prestart",
    "postbuild": "npm run extract-intl && npm run remove-sourcemaps && npm run zip-build",
    "build": "cross-env NODE_ENV=production webpack --config ./internals/webpack/webpack.prod.babel.js --color --mode production --progress",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true webpack --config ./internals/webpack/webpack.prod.babel.js --color --mode production --progress",
    "build:clean": "rimraf ./dist build.zip",
    "start": "cross-env NODE_ENV=development node server",
    "prestart": "node ./internals/scripts/babel-node ./internals/scripts/sync-code-with-config.js",
    "setup": "node ./internals/scripts/app-setup.js",
    "generate": "plop --plopfile ./internals/generators/index.js",
    "remove-sourcemaps": "node ./internals/scripts/remove-sourcemaps.js",
    "zip-build": "node ./internals/scripts/babel-node ./internals/scripts/zip-build.js",
    "lint": "npm run lint:js",
    "lint:css": "stylelint './app/**/*.js'",
    "lint:eslint": "eslint --fix",
    "lint:eslint:fix": "eslint --fix",
    "lint:js": "npm run lint:eslint -- app",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write",
    "test:unit": "cross-env NODE_ENV=test jest -w='4' --coverage --logHeapUsage --config ./internals/testing/jest.unit.config.js",
    "test:integration": "TZ='UTC' jest --coverage --logHeapUsage --config ./internals/testing/jest.integration.config.js --runInBand",
    "test": "npm run test:integration && npm run test:unit"
  },
  "jestSonar": {
    "reportPath": "reports/coverage",
    "reportFile": "test-reporter.xml",
    "indent": 2
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@capillarytech/vulcan-react-sdk": "^2.4.1",
    "connected-react-router": "4.5.0",
    "ml-matrix": "6.5.0",
